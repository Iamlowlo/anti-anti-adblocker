{
  "name": "anti-anti-ad",
  "description": "remove anti ad-blocker pop-up from websites",
  "version": "1.5",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["removeAntiAdBlocker.js"]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": ["recoverSignInContent.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
